<li class="timeline-item">
    <div class="timeline-info"><span>{{ timeline.data.date | postDate }}</span></div>
    <div class="timeline-marker"></div>
    <div class="timeline-content">
        <a href="{{ timeline.data.url }}" target="_blank" rel="noreferrer noopener nofollow">{{ timeline.data.title }}</a>
    </div>
</li>